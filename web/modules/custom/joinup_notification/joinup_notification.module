<?php

/**
 * @file
 * Contains joinup_notification.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function joinup_notification_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the joinup_notification module.
    case 'help.page.joinup_notification':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Handles notifications for joinup project.') . '</p>';
      return $output;

    default:
  }
}
