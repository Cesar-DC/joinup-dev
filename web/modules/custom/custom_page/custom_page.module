<?php
/**
 * @file
 * Main functions and hook implementations of the Custom Page module.
 */

use \Drupal\Core\Form\FormStateInterface;
use \Drupal\Core\Access\AccessResult;

/**
 * Implements hook_form_FORM_ID_alter().
 */
function custom_page_form_node_custom_page_edit_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  $form['og_group_ref']['#access']
    = AccessResult::allowedIfHasPermission(\Drupal::currentUser()->getAccount(),
      'moderate entity parents');
}
