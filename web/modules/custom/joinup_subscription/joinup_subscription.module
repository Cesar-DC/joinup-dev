/**
 * Implements hook_entity_type_alter().
 */
function joinup_subscription_entity_type_alter(array &$entity_types) {
  /** @var \Drupal\Core\Entity\EntityTypeInterface[] $entity_types */
  $entity_types['user']->setFormclass('subscription_settings', 'Drupal\user\Form\SubscriptionSettingsForm');
}
