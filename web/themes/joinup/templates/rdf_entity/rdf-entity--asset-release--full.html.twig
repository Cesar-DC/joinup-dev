{#
/**
 * @file
 * Themeing for rdf entities.
 */
#}
<article{{ attributes }}>
  {{ title_prefix }}
  {% if not page %}
    <h2{{ title_attributes }}>
      <a href="{{ url }}" rel="bookmark">{{ label }}</a>
    </h2>
  {% endif %}
  {{ title_suffix }}

  <div class="page__title-wrapper">
    <h1 class="page__title">{{ full_label }}</h1>
    {% if is_latest_release %}
      <div class="etiquette etiquette--blue">{{ 'Latest release'|t }}</div>
    {% endif %}
    <div class="details details--solution">
      <div class="details__element">
        <div class="value gray underline">dc9ee36</div>
      </div>
      <div class="details__element">
        <div class="value gray">{{ content.field_isr_creation_date }}</div>
      </div>
    </div>
  </div>

  <div class="page__content">
    {{ content.field_isr_release_notes}}
    <div class="action-link__wrapper right">
      <a href="/" class="action-link">
        <span class="action-link__icon icon icon--share"></span>
        <span class="action-link__link">{{ 'Share'|t }}</a>
      </a>
    </div>
  </div>
{% if content.field_isr_distribution|render %}
  <div class="listing listing--grid">
    <div class="row">
      <div class="mdl-grid">
        <div class="listing__header mdl-cell mdl-cell--12-col">
          <h3 class="listing__block-title">{{ 'Distributions'|t }}:</h3>
        </div>
          {{ content.field_isr_distribution }}
        </div>
      </div>
    </div>
  {% endif %}

</article>
